# URL to access ci services, e.g. example.domain.com
export WEB_SERVER_NAME=ci.infra.local

# Data volume root dir
export VOLUME_PATH=/srv/ci-infra

# Define OAUTH user authentication type in Gerrit
# export AUTH_TYPE=OAUTH
# use DEVELOPMENT_BECOME_ANY_ACCOUNT in a development environment
export AUTH_TYPE=DEVELOPMENT_BECOME_ANY_ACCOUNT

# Parameters for OAUTH
export OAUTH_ALLOW_EDIT_FULL_NAME=true
export OAUTH_ALLOW_REGISTER_NEW_EMAIL=true
export OAUTH_GOOGLE_RESTRICT_DOMAIN=your.site.domain
export OAUTH_GOOGLE_CLIENT_ID=sdmdmdigiesl-googleid
export OAUTH_GOOGLE_CLIENT_SECRET=asdadada-asaasscs-googlesecret
export OAUTH_GOOGLE_LINK_OPENID=true
export OAUTH_GITHUB_CLIENT_ID=client-id
export OAUTH_GITHUB_CLIENT_SECRET=secret_client

